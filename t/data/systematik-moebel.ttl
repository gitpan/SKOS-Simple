    <http://purl.org/dc/elements/1.1/identifier> "00000814" ;
    <http://purl.org/dc/qualifier/1.0/created> "2003-04-07" ;
    a <http://www.w3.org/2004/02/skos/core#Concept> ;
    <http://www.w3.org/2004/02/skos/core#broader> <http://www.museumsvokabular.de/museumvok/gefaess/00000813> ;
    <http://www.w3.org/2004/02/skos/core#definition> "Das Drehregal ist ein auf der Mittelachse drehbar gelagertes Regal." ;
    <http://www.w3.org/2004/02/skos/core#inScheme> "moebel" ;
    <http://www.w3.org/2004/02/skos/core#prefLabel> "Drehregal"@de .

<http://www.museumsvokabular.de/museumvok/moebel/00000815>
